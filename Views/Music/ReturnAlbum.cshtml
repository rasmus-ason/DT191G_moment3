@model Dt191G_moment3.Models.Music

@{
    ViewData["Title"] = "Lämna tillbaka album";

    @* var id = ViewContext.RouteData.Values["id"] as string; *@


}

<h1>Lämna tillbaka album</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="ReturnAlbum">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            
            <div class="form-group">
                <label asp-for="Title" class="control-label"></label>
                <input readonly asp-for="Title" class="form-control" />
                <span asp-validation-for="Title" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Artist" class="control-label"></label>
                <input readonly asp-for="Artist" class="form-control" />
                <span asp-validation-for="Artist" class="text-danger"></span>
            </div>

            <input type="hidden" asp-for="Tracks" class="form-control" />

            <br>
            <div class="form-group">
                <input type="submit" value="Lämna tillbaka" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Tillbaka</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}



